{
  "name": "aa-front-boilerplate",
  "version": "1.1.6",
  "description": "Boilerplate for web based on NextJS unversal framework",
  "main": "server/index.js",
  "scripts": {
    "setup": "rm -rf .git && git init",
    "build": "yarn build:css & next build",
    "dev": "yarn dev:css & node server",
    "build:css": "node scripts/buildCss",
    "dev:css": "yarn build:css --dev",
    "start": "pm2 start pm2.config.js --env production",
    "stop": "pm2 stop all && pm2 delete aa-front-boilerplate",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "artem@abzanov.com",
  "license": "MIT",
  "dependencies": {
    "compression": "1.7.1",
    "express": "4.16.2",
    "helmet": "3.11.0",
    "immutability-helper": "2.6.4",
    "isomorphic-unfetch": "2.0.0",
    "lru-cache": "4.1.1",
    "next": "4.2.3",
    "next-redux-wrapper": "1.3.5",
    "nprogress": "0.2.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-promise-middleware": "5.0.0",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "8.0.0",
    "babel-plugin-module-resolver": "3.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-env": "1.6.1",
    "cssnano": "3.10.0",
    "node-sass": "4.7.2",
    "postcss": "6.0.17",
    "postcss-discard-comments": "2.0.4",
    "postcss-loader": "2.1.0",
    "postcss-modules": "1.1.0",
    "redux-devtools-extension": "2.13.2",
    "sass-loader": "6.0.6",
    "skeleton-loader": "1.1.4"
  },
  "engines": {
    "node": ">=8.9.1",
    "yarn": ">=1.3.2"
  },
  "browserslist": [
    "last 2 versions",
    ">1%"
  ]
}
